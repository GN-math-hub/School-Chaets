    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>gn-math</title>
    <meta name="title" content="GN-Math" />
    <meta name="description" content="Play unbl*cked games like Crazy Cattle 3D and DriveMad on GN-Math. Fast, free, no downloads‚Äîperfect for school or home." />
    <style>
        /* --- Space Themed CSS --- */
        @import url('https://fonts.googleapis.com/css2?family=Press+Start+2P&display=swap');

        :root {
            --primary: #0ff; /* Neon cyan */
            --primary-hover: #f0f; /* Neon magenta */
            --primary-light: rgba(0, 255, 255, 0.1);
            --primary-lighter: rgba(0, 255, 255, 0.05);
            --accent: #f0f; /* Neon magenta */
            --accent-light: rgba(255, 0, 255, 0.1);
            --success: #0f0; /* Neon green */
            --warning: #ff0; /* Neon yellow */
            --text: #0ff; /* Neon cyan text */
            --text-muted: #555; /* Dimmer text */
            --text-light: #888;
            --bg: #000; /* Space black background */
            --bg-secondary: #111;
            --surface: #222;
            --surface-hover: #333;
            --border: #0ff; /* Neon border */
            --border-light: #0cc;
            --glass: rgba(0, 0, 0, 0.8);
            --shadow-xs: 0 0 2px #0ff;
            --shadow-sm: 0 0 5px #0ff, inset 0 0 10px #0ff;
            --shadow: 0 0 10px #0ff, inset 0 0 10px #0ff;
            --shadow-lg: 0 0 20px #0ff, inset 0 0 20px #0ff;
            --shadow-xl: 0 0 30px #0ff, inset 0 0 30px #0ff;
            --radius: 12px;
            --radius-sm: 8px;
            --radius-lg: 16px;
            --radius-xl: 20px;
            --gradient-primary: linear-gradient(135deg, #0ff, #f0f);
            --gradient-accent: linear-gradient(135deg, #f0f, #f0f);
            --gradient-surface: linear-gradient(135deg, #000, #222);
        }

        /* Flickering static noise overlay for space static effect */
        body {
            font-family: 'Press Start 2P', cursive, sans-serif;
            line-height: 1.6;
            margin: 0;
            padding: 0;
            background: 
                repeating-linear-gradient(
                    0deg,
                    rgba(0, 0, 0, 0.04),
                    rgba(0, 0, 0, 0.04) 1px,
                    transparent 1px,
                    transparent 2px
                ),
                url('https://www.transparenttextures.com/patterns/noise.png');
            background-blend-mode: overlay;
            animation: flicker 1.5s infinite alternate;
            color: var(--text);
            font-weight: 400;
            -webkit-font-smoothing: antialiased;
            -moz-osx-font-smoothing: grayscale;
        }

        /* Flicker animation for static noise effect */
        @keyframes flicker {
            0% { opacity: 0.97; }
            100% { opacity: 1; }
        }

        /* Header with glitch and flickering space nebula effect */
        header {
            background: repeating-linear-gradient(
                45deg,
                #000,
                #000 2px,
                #222 2px,
                #222 4px
            );
            color: var(--primary);
            font-family: 'Press Start 2P', cursive;
            position: sticky;
            top: 0;
            z-index: 100;
            box-shadow: none;
            backdrop-filter: none;
            animation: glitch 2s infinite alternate-reverse;
            padding: 0.5rem 1rem;
        }

        /* Glitch animation for header to simulate space static glitching */
        @keyframes glitch {
            0% {
                transform: translate(0);
                clip: rect(0, 100%, 0, 0);
            }
            20% {
                transform: translate(-2px, 0);
                clip: rect(0, 100%, 100%, 0);
            }
            40% {
                transform: translate(2px, 0);
                clip: rect(0, 100%, 100%, 0);
            }
            60% {
                transform: translate(-2px, 0);
                clip: rect(0, 100%, 0, 0);
            }
            80% {
                transform: translate(2px, 0);
                clip: rect(0, 100%, 100%, 0);
            }
            100% {
                transform: translate(0);
                clip: rect(0, 100%, 0, 0);
            }
        }

        /* Header content layout */
        .header-content {
            max-width: 1200px;
            margin: 0 auto;
            padding: 1rem;
            display: flex;
            align-items: center;
            justify-content: space-between;
            gap: 1rem;
        }

        /* Logo with neon glow effect */
        .logo {
            font-size: 1.75rem;
            font-weight: 800;
            letter-spacing: -0.02em;
            background: linear-gradient(135deg, #0ff, #f0f);
            -webkit-background-clip: text;
            -webkit-text-fill-color: transparent;
            background-clip: text;
            text-shadow: 0 0 5px #0ff, 0 0 10px #0ff, 0 0 20px #0ff;
            animation: flickerOverlay 2s infinite;
        }

        /* Search container styling */
        .search-container {
            display: flex;
            flex: 1;
            max-width: 500px;
            gap: 0.75rem;
        }

        /* Search input with glow and flicker */
        #searchBar {
            flex: 1;
            padding: 0.875rem 1rem;
            border: 0;
            border-radius: var(--radius);
            background: #222;
            color: var(--text);
            font-family: 'Press Start 2P', cursive;
            font-size: 15px;
            outline: none;
            box-shadow: 0 0 10px #0ff, inset 0 0 10px #0ff;
            transition: all 0.3s ease;
        }
        #searchBar:focus {
            background: #111;
            box-shadow: 0 0 20px #0ff, inset 0 0 20px #0ff;
            transform: translateY(-1px);
            color: #0ff;
        }
        #searchBar::placeholder {
            color: #555;
        }

        /* Sorting options styling with neon glow */
        #sortOptions {
            padding: 0.875rem 1rem;
            border: 0;
            border-radius: var(--radius);
            background: #222;
            color: #0ff;
            font-family: 'Press Start 2P', cursive;
            font-size: 15px;
            cursor: pointer;
            outline: none;
            transition: all 0.3s ease;
        }
        #sortOptions:hover {
            background: #111;
            box-shadow: 0 0 20px #0ff, inset 0 0 20px #0ff;
            color: #f0f;
        }

        /* Control buttons with glow effect */
        .control-buttons {
            display: flex;
            gap: 0.5rem;
        }
        #settings {
            background: #222;
            border: 0;
            color: #0ff;
            font-size: 1.2rem;
            cursor: pointer;
            padding: 0.75rem;
            border-radius: var(--radius);
        }
        #settings:hover {
            background: #111;
            box-shadow: 0 0 20px #0ff, inset 0 0 20px #0ff;
        }

        /* Main area styling */
        main {
            margin: 0 auto;
            padding: 2rem 1rem;
            padding-bottom: 6rem;
        }

        /* Zones count */
        #zoneCount {
            margin: 0 0 1.5rem;
            font-size: 14px;
            color: #555;
            font-weight: 600;
            display: flex;
            align-items: center;
            gap: 0.5rem;
        }

        /* Zones grid container */
        #container, #featuredZones {
            display: grid;
            grid-template-columns: repeat(auto-fill, minmax(200px, 1fr));
            gap: 1.5rem;
        }

        /* Each zone item style with neon glow and glitch effects */
        .zone-item {
            background: #111;
            border: 1px solid #0ff;
            border-radius: var(--radius-lg);
            overflow: hidden;
            cursor: pointer;
            transition: all 0.4s ease;
            display: flex;
            flex-direction: column;
            position: relative;
            box-shadow: 0 0 10px #0ff, inset 0 0 10px #0ff;
        }
        .zone-item:hover {
            transform: translateY(-8px) scale(1.02);
            box-shadow: 0 0 20px #0ff, inset 0 0 20px #0ff;
            border-color: #f0f;
        }
        .zone-item:hover::before {
            opacity: 1;
        }
        .zone-item::before {
            content: '';
            position: absolute;
            inset: 0;
            background: var(--primary-light);
            opacity: 0;
            transition: opacity 0.4s ease;
            pointer-events: none;
            z-index: 1;
        }
        .zone-item img {
            width: 100%;
            aspect-ratio: 1;
            object-fit: cover;
            display: block;
            background: #222;
            transition: transform 0.4s ease;
        }
        .zone-item:hover img {
            transform: scale(1.05);
        }
        .zone-item button {
            background: transparent;
            color: #0ff;
            border: 0;
            padding: 1.25rem 1rem;
            cursor: pointer;
            font-weight: 600;
            font-size: 14px;
            line-height: 1.4;
            text-align: center;
            transition: all 0.3s ease;
            min-height: 70px;
            display: flex;
            align-items: center;
            justify-content: center;
            position: relative;
            z-index: 2;
        }
        .zone-item:hover button {
            color: #f0f;
            background: rgba(0, 255, 255, 0.05);
        }

        /* Zone viewer overlay with glitch neon glow */
        #zoneViewer {
            position: fixed;
            inset: 0;
            background: #000;
            z-index: 1000;
            display: none;
            flex-direction: column;
            animation: slideInUp 0.3s ease;
        }
        @keyframes slideInUp {
            from { transform: translateY(100%); }
            to { transform: translateY(0); }
        }

        /* Zone header inside viewer with neon glitch effect */
        .zone-header {
            background: #000;
            color: #0ff;
            padding: 1rem 1.5rem;
            display: flex;
            align-items: center;
            justify-content: space-between;
        }
        .zone-title {
            flex: 1;
            min-width: 0;
        }
        #zoneName {
            font-size: 1.25rem;
            font-weight: 700;
            margin: 0 0 0.25rem;
            line-height: 1.3;
            text-shadow: 0 0 5px #0ff, 0 0 10px #0ff, 0 0 20px #0ff;
        }
        #zoneId {
            display: none;
        }
        #zoneAuthor {
            font-size: 14px;
            color: #555;
            text-decoration: none;
        }
        #zoneAuthor:hover {
            color: #0ff;
            text-decoration: underline;
        }
        .zone-controls {
            display: flex;
            gap: 0.75rem;
        }
        .zone-controls button {
            background: #222;
            border: 1px solid #0ff;
            color: #0ff;
            padding: 0.625rem 1rem;
            border-radius: var(--radius);
            cursor: pointer;
            font-size: 14px;
            font-weight: 600;
            transition: all 0.3s ease;
        }
        .zone-controls button:hover {
            background: #111;
            border-color: #f0f;
            box-shadow: 0 0 10px #0ff;
            transform: translateY(-1px);
        }

        /* iframe in viewer */
        #zoneFrame {
            flex-grow: 1;
            border: none;
            width: 90%;
            height: 90%;
        }

        /* Popup overlay styles with glitch neon effect */
        #popupOverlay {
            position: fixed;
            inset: 0;
            background: rgba(0, 0, 0, 0.8);
            backdrop-filter: blur(4px);
            display: none;
            align-items: center;
            justify-content: center;
            z-index: 2000;
            padding: 1rem;
        }
        @keyframes fadeIn {
            from { opacity: 0; }
            to { opacity: 1; }
        }
        .popup {
            background: #222;
            border-radius: var(--radius-lg);
            box-shadow: 0 0 20px #0ff, inset 0 0 20px #0ff;
            width: 100%;
            max-width: 520px;
            max-height: 80vh;
            overflow: hidden;
            display: flex;
            flex-direction: column;
            border: 1px solid #0ff;
            animation: slideInScale 0.3s ease;
        }
        @keyframes slideInScale {
            from {
                transform: scale(0.9) translateY(20px);
                opacity: 0;
            }
            to {
                transform: scale(1) translateY(0);
                opacity: 1;
            }
        }
        .popup-header {
            background: #000;
            color: #0ff;
            padding: 1.25rem 1.5rem;
            display: flex;
            justify-content: space-between;
            align-items: center;
        }
        #popupTitle {
            margin: 0;
            font-size: 1.25rem;
            font-weight: 700;
        }
        #popupClose {
            background: #222;
            border: 0;
            color: #0ff;
            font-size: 1.25rem;
            cursor: pointer;
            padding: 0.5rem;
        }
        #popupClose:hover {
            background: #111;
            transform: scale(1.1);
        }
        #popupBody {
            padding: 1.5rem;
            overflow-y: auto;
            color: #0ff;
        }

        /* Settings button styles with neon glow */
        #settings-button {
            width: 100%;
            padding: 0.875rem 1rem;
            background: #222;
            color: #0ff;
            border: 0;
            border-radius: var(--radius);
            cursor: pointer;
        }
        #settings-button:hover {
            transform: translateY(-2px);
            box-shadow: 0 0 20px #0ff, inset 0 0 20px #0ff;
        }

        /* Footer styles with neon glow */
        footer {
            background: #000;
            border-top: 1px solid #0ff;
            padding: 1rem;
            text-align: center;
            color: #0ff;
            font-family: 'Press Start 2P', cursive;
            font-size: 12px;
            position: fixed;
            bottom: 0;
            left: 0;
            width: 100%;
            z-index: 100;
            text-shadow: 0 0 5px #0ff, 0 0 10px #0ff;
        }

        /* Responsive adjustments */
        @media(max-width: 768px) {
            .header-content {
                flex-direction: column;
                gap: 1rem;
                padding: 1rem;
            }
            .search-container {
                width: 100%;
                max-width: none;
            }
            main {
                padding: 1.5rem 1rem;
            }
            #container, #featuredZones {
                grid-template-columns: repeat(auto-fill, minmax(160px, 1fr));
                gap: 1.25rem;
            }
        }
        @media(max-width: 480px) {
            #container, #featuredZones {
                grid-template-columns: repeat(auto-fill, minmax(140px, 1fr));
                gap: 1rem;
            }
            .popup {
                margin: 0.5rem;
            }
            .zone-item:hover {
                transform: translateY(-10px) scale(1.01);
            }
        }
    </style>
</head>
<body>
<header>
    <div class="header-content">
        <div class="logo">gn-math</div>
        <div class="search-container">
            <input type="text" id="searchBar" placeholder="Search zones..." oninput="filterZones()">
            <select id="sortOptions" onchange="sortZones()">
                <option value="name">Name</option>
                <option value="id">ID (Date)</option>
                <option value="popular">Popular</option>
            </select>
        </div>
        <div class="control-buttons">
            <button id="settings" style="background-color: var(--primary); color: white; border: none; border-radius: 4px; padding: 0.5rem 1rem; font-size: 16px; cursor: pointer;">
                <img src="setting.png" alt="settings" width="45px">
            </button>
        </div>
    </div>
</header>

<!-- Ad placeholder -->
<ins class="adsbygoogle" style="display:block; text-align:center;"
     data-ad-client="ca-pub-5521219086088837"
     data-ad-slot="5549138288"
     data-ad-format="auto"
     data-full-width-responsive="true"></ins>
<script>(adsbygoogle = window.adsbygoogle || []).push({});</script>

<main>
    <details id="featuredZonesWrapper" open>
        <summary id="allZonesSummary" style="font-size: 1.2rem; font-weight: bold; cursor: pointer;">Featured Zones</summary>
        <div id="featuredZones" class="zone-container"></div>
    </details>
    <br><hr><br>
    <details id="allZonesWrapper" open>
        <summary id="allSummary" style="font-size: 1.2rem; font-weight: bold; cursor: pointer;">All Zones</summary>
        <div id="container">Loading...</div>
    </details>
</main>

<!-- Zone viewer overlay -->
<div id="zoneViewer" style="display: none;">
    <div class="zone-header">
        <div class="zone-title">
            <h2 id="zoneName">zone</h2>
            <span id="zoneId" style="display: none;"></span>
            <a id="zoneAuthor" href="#" target="_blank">by Author</a>
        </div>
        <div class="zone-controls">
            <button onclick="fullscreenZone()">üé•</button>
            <button onclick="aboutBlank()">üåê</button>
            <button onclick="downloadZone()">üíæ</button>
            <button onclick="closeZone()">‚úñ</button>
        </div>
    </div>
    <iframe id="zoneFrame"></iframe>
</div>

<!-- Popup overlay -->
<div id="popupOverlay">
    <div class="popup">
        <div class="popup-header">
            <h3 id="popupTitle">Title</h3>
            <button id="popupClose" onclick="closePopup()">√ó</button>
        </div>
        <div id="popupBody">Content will be here</div>
    </div>
</div>

<!-- Scripts -->
<script>
  // Static example zones data for testing
  const zones = [
    {
      "name": "Space Zone 1",
      "id": 1,
      "cover": "https://images.unsplash.com/photo-1477958675604-1ed6e0f2f6f1",
      "url": "https://example.com/space-zone-1",
      "author": "NASA",
      "authorLink": "https://nasa.gov"
    },
    {
      "name": "Space Zone 2",
      "id": 2,
      "cover": "https://images.unsplash.com/photo-1506744038136-46273834b3fb",
      "url": "https://example.com/space-zone-2",
      "author": "ESA",
      "authorLink": "https://esa.int"
    }
  ];

  const coverURL = "https://cdn.jsdelivr.net/gh/gn-math/covers@main";
  const htmlURL = "https://cdn.jsdelivr.net/gh/gn-math/html@main";

  const searchBar = document.getElementById('searchBar');
  const sortOptions = document.getElementById('sortOptions');

  function displayZones(zones) {
    const container = document.getElementById('container');
    container.innerHTML = "";
    zones.forEach((file) => {
      const zoneItem = document.createElement("div");
      zoneItem.className = "zone-item";
      zoneItem.onclick = () => openZone(file);
      const img = document.createElement("img");
      img.dataset.src = file.cover; // use direct URL
      img.alt = file.name;
      img.loading = "lazy";
      img.className = "lazy-zone-img";
      zoneItem.appendChild(img);
      const button = document.createElement("button");
      button.textContent = file.name;
      button.onclick = (event) => {
        event.stopPropagation();
        openZone(file);
      };
      zoneItem.appendChild(button);
      container.appendChild(zoneItem);
    });
    document.getElementById("allSummary").textContent = `All Zones (${zones.length})`;
    // Lazy load images
    const lazyImages = document.querySelectorAll('img.lazy-zone-img');
    const observer = new IntersectionObserver((entries, obs) => {
      entries.forEach(entry => {
        if (entry.isIntersecting) {
          const img = entry.target;
          img.src = img.dataset.src;
          img.classList.remove("lazy-zone-img");
          observer.unobserve(img);
        }
      });
    }, { rootMargin: "100px", threshold: 0.1 });
    lazyImages.forEach(img => {
      observer.observe(img);
    });
  }

  // Initialize display
  displayZones(zones);

  function openZone(file) {
    if (file.url.startsWith("http")) {
      window.open(file.url, "_blank");
    } else {
      // fallback if needed
    }
  }

  function closeZone() {
    document.getElementById('zoneViewer').style.display = "none";
  }

  function aboutBlank() {
    const newWindow = window.open("about:blank", "_blank");
    let zone = zones.find(zone => zone.id + '' === document.getElementById('zoneId').textContent).url.replace("{COVER_URL}", coverURL).replace("{HTML_URL}", htmlURL);
    fetch(zone + "?t=" + Date.now())
      .then(response => response.text())
      .then(html => {
        if (newWindow) {
          newWindow.document.open();
          newWindow.document.write(html);
          newWindow.document.close();
        }
      });
  }

  function downloadZone() {
    let zone = zones.find(zone => zone.id + '' === document.getElementById('zoneId').textContent);
    fetch(zone.url.replace("{HTML_URL}", htmlURL) + "?t=" + Date.now())
      .then(res => res.text())
      .then(text => {
        const blob = new Blob([text], { type: "text/plain;charset=utf-8" });
        const url = URL.createObjectURL(blob);
        const a = document.createElement('a');
        a.href = url;
        a.download = zone.name + ".html";
        document.body.appendChild(a);
        a.click();
        document.body.removeChild(a);
        URL.revokeObjectURL(url);
      });
  }

  function fullscreenZone() {
    if (zoneFrame.requestFullscreen) {
      zoneFrame.requestFullscreen();
    } else if (zoneFrame.mozRequestFullScreen) {
      zoneFrame.mozRequestFullScreen();
    } else if (zoneFrame.webkitRequestFullscreen) {
      zoneFrame.webkitRequestFullscreen();
    } else if (zoneFrame.msRequestFullscreen) {
      zoneFrame.msRequestFullscreen();
    }
  }

  function closePopup() {
    document.getElementById('popupOverlay').style.display = "none";
  }
</script>
</body>
</html>
